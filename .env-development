# RoomJS game engine port and address to connect
PORT=8888
ADDRESS=127.0.0.1

# How verbose the log should be
# levels are: trace, debug, info, warn, error, fatal
LOG_LEVEL=info

# Bot user account
BOT_USER=dongann

# Bot password 
# CAVEAT -- DO NOT SET THIS HERE IN PRODUCTION !!!
# This is for illustration only, and convenience, but 
# always use an environment variable here. Don't
# expose the password in a file.
BOT_PASSWORD=dongann

# Bot character name
# Will be used if the account has multiple characters.
BOT_CHARACTER=Dongann

# Bot typing speed in Characters Per Minute
# 200-300 is fast for humans, but slow for bots...
BOT_CPM=800

# Bot inactivity timeout in ms
BOT_INACTIVITY=60000
